@{
    ViewBag.Title = "UpdateEvent";
}
@using SqliteDemo.Models.Entity;
@model Events

<h2>UpdateEvent</h2>

<table style="width:100%">
    <tr>
        <th>Event ID</th>
        <th>User ID</th>
        <th>Event Name</th>
    </tr>
    
    @{
            foreach (Events events in (List<Events>)ViewData["events"])
            {
                <tr>

                    <td>@events.EventId</td>
                    <td>@events.UserId</td>
                    <td>@events.EventName</td>

                </tr>
            }        
    }

    
    
</table>
<td>
<form method="post" action="~/Event/UpdateEvent">
    <button type="submit" value="Change" name="Change" class="btn btn-default">Change</button>

</form></td>
